<h1>Welcome to Event Scheduler</h1>

<% if current_user %>
  Logged in as <%= current_user.name.capitalize %>.
  <%= link_to "Log Out", logout_path %>
<% else %>
  <li><%= link_to "Log In", login_path %></li>
  <li><%= link_to "Sign Up", signup_path %></li>
<% end %>

<p id="notice"><%= notice %></p>